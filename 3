#include <iostream>
#include <cmath>
#include <Windows.h>
using namespace std;

int main()
{
 int n;
 cout << "Podaj liczbę ";
 cin >> n;
 while (n<0)
 {
 cout << "nie można obliczyć pierwiastka \n\nPodaj inne liczbę ";
 cin >> n;
 }
 cout << "pierwiastek liczby wynosi:" << sqrt(n) << endl;
 return 0;
    return 0;
}
